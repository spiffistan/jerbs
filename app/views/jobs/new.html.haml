%h1 Add job

= form_tag
= form_for(@job) do |f|
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :description
    = f.text_area :description
  .field
    = f.label :deadline
    = f.text_field :deadline
  .field
    = f.label :positions
    = f.text_field :positions
  .field
    = f.label :freedom
    = f.text_field :freedom
  =f.submit